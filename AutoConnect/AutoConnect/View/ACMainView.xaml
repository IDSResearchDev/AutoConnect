                            <TextBlock Grid.Row="0"
                                       Grid.Column="1"
                                       Width="Auto"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       FontWeight="Bold"
                                       Text="Objects" />
                            <TextBlock Grid.Row="0"
                                       Grid.Column="2"
                                       Width="Auto"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       FontWeight="Bold"
                                       Text="#" />
                            <TextBlock Grid.Row="0"
                                       Grid.Column="3"
                                       Width="Auto"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       FontWeight="Bold"
                                       Text="Component" />
                            <TextBlock Grid.Row="0"
                                       Grid.Column="4"
                                       Width="Auto"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       FontWeight="Bold"
                                       Text="Bolt/Welds" />
                            <TextBlock Grid.Row="0"
                                       Grid.Column="5"
                                       Width="Auto"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       FontWeight="Bold"
                                       Text="Angle" />

                            <CheckBox Grid.Row="1"
                                      Grid.Column="0"
                                      Margin="10,6,5,5"
                                      HorizontalAlignment="Stretch"
                                      VerticalAlignment="Center"
                                      HorizontalContentAlignment="Center"
                                      VerticalContentAlignment="Center"
                                      IsChecked="{Binding Included_BTCFS}">
                                <interact:Interaction.Triggers>
                                    <interact:EventTrigger EventName="Checked">
                                        <interact:InvokeCommandAction Command="{Binding ElementName=vm, Path=CheckBox_CheckChanged}" CommandParameter="{Binding Path=ConnectingObjects}" />
                                    </interact:EventTrigger>
                                    <interact:EventTrigger EventName="Unchecked">
                                        <interact:InvokeCommandAction Command="{Binding ElementName=vm, Path=CheckBox_CheckChanged}" />
                                    </interact:EventTrigger>
                                </interact:Interaction.Triggers>
                            </CheckBox>

                            <Image Grid.Row="1"
                                   Grid.Column="1"
                                   Width="Auto"
                                   Margin="5"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   Source="/AutoConnect;component/Resources/BeamToColumn Single.png"
                                   Stretch="Fill" />

                            <TextBlock Grid.Row="1"
                                       Grid.Column="2"
                                       Width="Auto"
                                       Margin="5"
                                       HorizontalAlignment="Stretch"
                                       VerticalAlignment="Center"
                                       Text="{Binding B2CF_CollectionCount}"
                                       TextAlignment="Center" />

                            <ComboBox Grid.Row="1"
                                      Grid.Column="3"
                                      Width="Auto"
                                      Height="30"
                                      Margin="5"
                                      HorizontalAlignment="Stretch"
                                      VerticalAlignment="Center"
                                      DisplayMemberPath="Description"
                                      ItemsSource="{Binding Source={converter:Enum {x:Type converter:ComponentType}}}"
                                      SelectedValue="{Binding B2CFComponentTypes}"
                                      SelectedValuePath="Value" />
                            <ComboBox Grid.Row="1"
                                      Grid.Column="4"
                                      Width="Auto"
                                      Height="30"
                                      Margin="5"
                                      HorizontalAlignment="Stretch"
                                      VerticalAlignment="Center"
                                      DisplayMemberPath="Description"
                                      ItemsSource="{Binding Source={converter:Enum {x:Type converter:BoltWeldOrientation}}}"
                                      SelectedValue="{Binding B2CFBoltWeldTypes}"
                                      SelectedValuePath="Value" />

                            <ComboBox Grid.Row="1"
                                      Grid.Column="5"
                                      Width="Auto"
                                      Height="30"
                                      Margin="5"
                                      HorizontalAlignment="Stretch"
                                      VerticalAlignment="Center"
                                      DisplayMemberPath="Description"
                                      ItemsSource="{Binding Source={converter:Enum {x:Type converter:AngleTypes}}}"
                                      SelectedValue="{Binding B2CFAngleTypes}"
                                      SelectedValuePath="Value" />

                        </Grid>
                    </StackPanel>
                </Grid>

            </StackPanel>
        </ScrollViewer>

    </DockPanel>
</Window>




