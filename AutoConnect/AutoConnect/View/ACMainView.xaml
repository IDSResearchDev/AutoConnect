                                      VerticalAlignment="Center"
                                      HorizontalContentAlignment="Center"
                                      VerticalContentAlignment="Center"
                                      FontWeight="Bold"
                                      IsChecked="{Binding Included_BTBWS}">

                                <interact:Interaction.Triggers>
                                    <interact:EventTrigger EventName="Checked">
