                            <TextBlock Grid.Row="0"
                                       Grid.Column="3"
                                       Width="Auto"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       FontWeight="Bold"
                                       Text="Component" />
                            <TextBlock Grid.Row="0"
                                       Grid.Column="4"
                                       Width="Auto"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       FontWeight="Bold"
                                       Text="Bolt/Welds" />
                            <TextBlock Grid.Row="0"
                                       Grid.Column="5"
                                       Width="Auto"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       FontWeight="Bold"
                                       Text="Angle" />

