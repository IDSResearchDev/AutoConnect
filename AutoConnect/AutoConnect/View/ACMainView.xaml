                            <TextBlock Grid.Row="0"
                                       Grid.Column="1"
                                       Width="Auto"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       FontWeight="Bold"
                                       Text="Objects" />
                            <TextBlock Grid.Row="0"
                                       Grid.Column="2"
                                       Width="Auto"
                                       HorizontalAlignment="Center"
                                       VerticalAlignment="Center"
                                       FontWeight="Bold"
